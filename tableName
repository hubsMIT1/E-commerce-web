CREATE TABLE users(
    id INT(11) NOT NULL AUTO_INCREMENT PRIMERY KEY,
    username VARCHAR(250) NOT NULL,
    useremail varchar(250) NOT NULL,
    pswrd varchar(250) NOT NULL,
    Cp varchar(250) NOT NULL,
    dt DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP
)
CREATE TABLE product(
    id2 INT(11) NOT NULL AUTO_INCREMENT PRIMARY KEY,
    pName varchar(250) NOT NULL,
    amount varchar(250) NOT NULL,
    pIMg varchar(250) NOT NULL,
    desc varchar(250) NOT NULL,
    userId INT(11) NOT NULL,
    dt2 DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP
)


CREATE TABLE bid_amount (
    id3 INT(11) NOT NULL AUTO_INCREMENT PRIMARY KEY,
    Pnm_b varchar(250) NOT NULL,
    amount INT(250) NOT NULL,
    pId INT(250) NOT NULL,
    uId INT(250) NOT NULL,
    dt3 DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP
)

CREATE TABLE bidl(
    id4 INT(11) NOT NULL AUTO_INCREMENT PRIMARY KEY,
    pIdB INT(250) NOT NULL AUTO_INCREMENT PRIMARY KEY,
    uIdB INT(250) NOT NULL AUTO_INCREMENT PRIMARY KEY,
    timeL INT(250) NOT NULL AUTO_INCREMENT PRIMARY KEY,
    mnth INT(250) NOT NULL AUTO_INCREMENT PRIMARY KEY,
    yr INT(250) NOT NULL AUTO_INCREMENT PRIMARY KEY,
    dt4 DATETIME NOT NULL DEFAULT CURRENT_TIMESTAMP

)
for Admin USERNAME = Admin;
